  "author": "MUI Team",
  "main": "src/index.ts",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mui/mui-x/issues"
  },
  "homepage": "https://github.com/mui/mui-x/tree/master/packages/x-data-grid-generator",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "keywords": [
    "mui-x"
