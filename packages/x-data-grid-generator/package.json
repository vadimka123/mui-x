  "repository": {
    "type": "git",
    "url": "https://github.com/mui/mui-x.git",
    "directory": "packages/x-data-grid-generator"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/x-data-grid-premium": "workspace:*",
    "chance": "^1.1.11",
    "clsx": "^2.1.0",
    "lru-cache": "^10.2.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/lru-cache": "^7.10.9",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.4.1",
